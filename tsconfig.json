{
    "compilerOptions": {
        "module": "CommonJS",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "target": "esnext",
        "noImplicitAny": true,
        "noEmit": false,
        "sourceMap": true,
        "outDir": "server-prod",
        "baseUrl": ".",
        "jsx": "react",
        "forceConsistentCasingInFileNames": true,
        "types": [
            "express",
            "node"
        ]
    },
    "ts-node": {
        "compilerOptions": {
            "module": "CommonJS"
        }
    },
    "exclude": [
        "./server-prod",
        "./.parcel-cache",
        "./node_modules/*"
    ]
}
